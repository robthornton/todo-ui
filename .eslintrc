{
  "extends": "react",
  "plugins": [
    "react",
    "@typescript-eslint",
    "prettier",
    "prettier/@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "prettier/prettier": "error",
    "eol-last": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
